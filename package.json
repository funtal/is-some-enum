    "is-some-enum",
    "isSomeEnumOf",
    "isSomeEnumOfType",
    "isSomeEnumOfTypeSafe",
    "is-some-enum-type-guard",
    "type-guard-for-string-enum",
    "isSomeEnumOfTypeForStringEnum",
    "is-some-enum-type-guard-for-string-enum"
